<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
        <from>^index.html$</from>
        <to type="redirect">/pages/index.html$1</to>
    </rule>
    <outbound-rule>
        <from>^/(.*).html$</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>